{
  "name": "Mint Chocolate",
  "type": "dark",
  "semanticHighlighting": true,
  "colors": {
    "foreground": "#c9d5d0",
    "focusBorder": "#37B39040",
    
    // 기본 UI
    "editor.background": "#302721",
    "editor.foreground": "#c9d5d0",
    
    // Activity Bar
    "activityBar.background": "#302721",
    "activityBar.foreground": "#c9d5d0",
    "activityBar.inactiveForeground": "#7A655E",
    "activityBar.border": "#241D1A",
    "activityBar.activeBorder": "#37B390",
    "activityBar.activeBackground": "#241D1A",
    "activityBarBadge.background": "#37B390",
    "activityBarBadge.foreground": "#302721",

    // Side Bar
    "sideBar.background": "#302721",
    "sideBar.foreground": "#c9d5d0",
    "sideBar.border": "#241D1A",
    "sideBarTitle.foreground": "#c9d5d0",
    "sideBarSectionHeader.background": "#241D1A",
    "sideBarSectionHeader.foreground": "#c9d5d0",
    "sideBarSectionHeader.border": "#37B39040",

    // Editor Groups & Tabs
    "editorGroup.border": "#241D1A",
    "editorGroup.dropBackground": "#37B39020",
    "editorGroupHeader.tabsBackground": "#302721",
    "tab.activeBackground": "#241D1A",
    "tab.activeForeground": "#c9d5d0",
    "tab.inactiveBackground": "#302721",
    "tab.inactiveForeground": "#7A655E",
    "tab.hoverBackground": "#37B39020",
    "tab.border": "#241D1A",
    "tab.activeBorderTop": "#37B390",

    // Status Bar
    "statusBar.background": "#302721",
    "statusBar.foreground": "#c9d5d0",
    "statusBar.border": "#241D1A",
    "statusBar.debuggingBackground": "#37B390",
    "statusBar.debuggingForeground": "#302721",
    "statusBar.noFolderBackground": "#302721",
    "statusBarItem.remoteBackground": "#37B390",
    "statusBarItem.remoteForeground": "#302721",

    // Editor Decorations
    "editor.lineHighlightBackground": "#241D1A",
    "editor.lineHighlightBorder": "#241D1A",
    "editor.selectionBackground": "#37B39040",
    "editor.selectionHighlightBackground": "#37B39020",
    "editor.wordHighlightBackground": "#37B39030",
    "editor.wordHighlightStrongBackground": "#37B39050",
    "editor.findMatchBackground": "#E5B08860",
    "editor.findMatchHighlightBackground": "#E5B08830",
    "editor.findRangeHighlightBackground": "#37B39020",
    "editor.hoverHighlightBackground": "#37B39015",
    "editor.lineNumberForeground": "#7A655E",
    "editor.lineNumberActiveForeground": "#c9d5d0",
    "editorLineNumber.foreground": "#7A655E",
    "editorLineNumber.activeForeground": "#c9d5d0",
    "editorCursor.foreground": "#37B390",
    "editorWhitespace.foreground": "#4D3C3380",
    "editorIndentGuide.background": "#4D3C3340",
    "editorIndentGuide.activeBackground": "#4D3C3380",
    
    // Breadcrumbs
    "breadcrumb.foreground": "#7A655E",
    "breadcrumb.focusForeground": "#c9d5d0",
    "breadcrumb.activeSelectionForeground": "#37B390",
    "breadcrumbPicker.background": "#241D1A",

    // Settings
    "settings.headerForeground": "#c9d5d0",
    "settings.modifiedItemIndicator": "#37B390",
    "settings.dropdownBackground": "#241D1A",
    "settings.dropdownForeground": "#c9d5d0",
    "settings.dropdownBorder": "#37B39040",
    "settings.checkboxBackground": "#241D1A",
    "settings.checkboxForeground": "#c9d5d0",
    "settings.checkboxBorder": "#37B39040",
    
    // Terminal Colors
    "terminal.background": "#302721",
    "terminal.foreground": "#c9d5d0",
    "terminal.ansiBlack": "#241D1A",
    "terminal.ansiRed": "#E67A73",
    "terminal.ansiGreen": "#84D4BC",
    "terminal.ansiYellow": "#E5B088",
    "terminal.ansiBlue": "#66CFB9",
    "terminal.ansiMagenta": "#CE8179",
    "terminal.ansiCyan": "#37B390",
    "terminal.ansiWhite": "#c9d5d0",
    "terminal.ansiBrightBlack": "#4D3C33",
    "terminal.ansiBrightRed": "#E67A73",
    "terminal.ansiBrightGreen": "#84D4BC",
    "terminal.ansiBrightYellow": "#E5B088",
    "terminal.ansiBrightBlue": "#66CFB9",
    "terminal.ansiBrightMagenta": "#CE8179",
    "terminal.ansiBrightCyan": "#45C4A0",
    "terminal.ansiBrightWhite": "#F5FAF8",
    "terminal.tab.activeBorder": "#37B390",

    // Git Colors
    "gitDecoration.addedResourceForeground": "#84D4BC",
    "gitDecoration.modifiedResourceForeground": "#E5B088",
    "gitDecoration.deletedResourceForeground": "#E67A73",
    "gitDecoration.untrackedResourceForeground": "#37B390",
    "gitDecoration.ignoredResourceForeground": "#7A655E",
    "gitDecoration.conflictingResourceForeground": "#CE8179",
    
    // Notifications
    "notificationCenter.border": "#241D1A",
    "notifications.background": "#241D1A",
    "notifications.foreground": "#c9d5d0",
    "notifications.border": "#37B39040",
    "notificationLink.foreground": "#37B390",
    
    // Extension Button
    "extensionButton.prominentBackground": "#37B390",
    "extensionButton.prominentForeground": "#302721",
    "extensionButton.prominentHoverBackground": "#45C4A0",

    // List Colors
    "list.activeSelectionBorder": "#37B390",          // 활성 선택 항목 테두리
    "list.focusBorder": "#37B39080",                  // 포커스된 항목 테두리
    "list.highlightForeground": "#84D4BC",            // 하이라이트된 항목 텍스트
    "list.inactiveSelectionBorder": "#37B39040",      // 비활성 선택 항목 테두리
    "list.activeSelectionBackground": "#37B39020",    // 활성 선택 항목 배경
    "list.activeSelectionForeground": "#313433",      // 활성 선택 항목 텍스트
    "list.inactiveSelectionBackground": "#37B39010",  // 비활성 선택 항목 배경
    "list.inactiveSelectionForeground": "#c9d5d0",    // 비활성 선택 항목 텍스트
    "list.hoverBackground": "#37B39010",              // 마우스 오버 시 배경
    "list.focusHighlightForeground": "#84D4BC",       // 포커스된 항목의 하이라이트 텍스트
    "list.focusOutline": "#37B39040",                 // 포커스 아웃라인

    // Peek View
    "peekView.border": "#37B390",
    "peekViewEditor.background": "#302721",
    "peekViewTitle.background": "#241D1A",
    "peekViewResult.background": "#241D1A",
    "peekViewResult.selectionBackground": "#37B39040"
  },
  "tokenColors": [
    {
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#7A655E",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["string", "string.template"],
      "settings": {
        "foreground": "#84D4BC"
      }
    },
    {
      "scope": ["constant.numeric", "constant.language", "constant.other"],
      "settings": {
        "foreground": "#E5B088"
      }
    },
    {
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#37B390"
      }
    },
    // React/JSX specific
    {
      "scope": ["entity.name.tag.jsx", "entity.name.tag.tsx", "punctuation.definition.tag"],
      "settings": {
        "foreground": "#f29c89"
      }
    },
    {
      "scope": ["entity.other.attribute-name.jsx", "entity.other.attribute-name.tsx"],
      "settings": {
        "foreground": "#45C4A0",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "meta.jsx.children",
      "settings": {
        "foreground": "#c9d5d0"
      }
    },
    // TypeScript specific
    {
      "scope": ["entity.name.type.interface", "entity.name.type.type"],
      "settings": {
        "foreground": "#66CFB9",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "entity.name.type.enum",
      "settings": {
        "foreground": "#E5B088",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["support.type.primitive", "support.type.builtin"],
      "settings": {
        "foreground": "#37B390"
      }
    },
    {
      "scope": "meta.type.annotation",
      "settings": {
        "foreground": "#84D4BC"
      }
    },
    {
      "scope": ["entity.name.type.generic", "meta.type.parameters"],
      "settings": {
        "foreground": "#66CFB9"
      }
    },
    // Function and Class
    {
      "scope": "entity.name.function",
      "settings": {
        "foreground": "#45C4A0"
      }
    },
    {
      "scope": ["entity.name.type", "entity.name.class", "entity.name.namespace"],
      "settings": {
        "foreground": "#66CFB9",
        "fontStyle": "bold"
      }
    },
    // Variables and Parameters
    {
      "scope": ["variable", "variable.other"],
      "settings": {
        "foreground": "#c9d5d0"
      }
    },
    {
      "scope": "variable.parameter",
      "settings": {
        "foreground": "#C4A886",
        "fontStyle": "italic"
      }
    },
    // Decorators
    {
      "scope": ["meta.decorator", "meta.decorator punctuation.decorator"],
      "settings": {
        "foreground": "#E5B088",
        "fontStyle": "italic"
      }
    },
    // Import/Export
    {
      "scope": ["keyword.control.import", "keyword.control.export"],
      "settings": {
        "foreground": "#37B390",
        "fontStyle": "bold"
      }
    },
    // React Hooks
    {
      "scope": "support.function.hooks",
      "settings": {
        "foreground": "#45C4A0",
        "fontStyle": "italic"
      }
    },
    // Template Literals
    {
      "scope": ["string.template", "punctuation.definition.template-expression"],
      "settings": {
        "foreground": "#84D4BC"
      }
    },
    {
      "scope": "meta.template.expression",
      "settings": {
        "foreground": "#c9d5d0"
      }
    },
    // Object Properties
    {
      "scope": "meta.object-literal.key",
      "settings": {
        "foreground": "#45C4A0"
      }
    },
    // Support Functions
    {
      "scope": ["support.function", "support.class"],
      "settings": {
        "foreground": "#66CFB9"
      }
    },
    // Markdown
    {
      "scope": ["markup.heading", "markup.heading entity.name"],
      "settings": {
        "foreground": "#37B390",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "markup.quote",
      "settings": {
        "foreground": "#84D4BC",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "markup.bold",
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "scope": "markup.italic",
      "settings": {
        "fontStyle": "italic"
      }
    }
  ]
}