  "tokenColors": [
    {
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#7A655E",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["string", "string.template"],
      "settings": {
        "foreground": "#84D4BC"
      }
    },
    {
      "scope": ["constant.numeric", "constant.language", "constant.other"],
      "settings": {
        "foreground": "#E5B088"
      }
    },
    {
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#37B390"
      }
    },
    {
      "scope": ["entity.name.tag.jsx", "entity.name.tag.tsx", "punctuation.definition.tag"],
      "settings": {
        "foreground": "#f29c89"
      }
    },
    {
      "scope": ["entity.other.attribute-name.jsx", "entity.other.attribute-name.tsx"],
      "settings": {
        "foreground": "#45C4A0",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["entity.name.type.interface", "entity.name.type.type"],
      "settings": {
        "foreground": "#66CFB9",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["meta.tag", "meta.brace"],
      "settings": {
        "foreground": "#c9d5d0"
      }
    },
    {
      "scope": ["support.type.primitive", "support.type.builtin"],
      "settings": {
        "foreground": "#37B390"
      }
    },
    {
      "scope": ["variable", "variable.other"],
      "settings": {
        "foreground": "#c9d5d0"
      }
    },
    {
      "scope": "variable.parameter",
      "settings": {
        "foreground": "#C4A886",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["support.function", "support.class"],
      "settings": {
        "foreground": "#66CFB9"
      }
    },
    {
      "scope": ["markup.heading", "markup.heading entity.name"],
      "settings": {
        "foreground": "#37B390",
        "fontStyle": "bold"
      }
    }
  ]
}